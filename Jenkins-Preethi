pipeline {
      agent any
      
      stages{
            stage("SCMcheckout"){
            steps{
                  git credentialsId: '288ed8a9-f81c-4b30-adfa-5432392201e9', url: 'https://github.com/RAJANI9/myweb.git'
                  }
            }
            
            stage("build"){
                  steps{
                       sh "mvn clean package"
                       sh "mv target/*.war target/myweb.war"
                       }
                       }
                       stage("depoly"){
                  steps{
                       deploy adapters: [tomcat9(credentialsId: 'c99bf467-a1fa-4788-893a-cea3abae32dc', path: '', url: 'http://3.6.86.20:8080/')], contextPath: 'myweb', war: 'target/*.war'
                       }
            }
       }
       }
